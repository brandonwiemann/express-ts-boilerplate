{
  "name": "express-ts-boilerplate",
  "version": "0.1.0",
  "description": "An opinionated express/typescript boilerplate project for building RESTful backends",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint src/*/**.ts",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon"
  },
  "author": "Brandon Wiemann <me@brandonwiemann.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^3.1.47",
    "@types/express-session": "^1.15.15",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/passport-auth0": "^1.0.2",
    "@types/pino": "^6.3.0",
    "@types/redis": "^2.8.14",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "bind-decorator": "^1.0.11",
    "eslint": "^6.6.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/faker": "^4.1.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.0",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-jwt-cookiecombo": "^1.0.5",
    "passport-local": "^1.0.0",
    "pino": "^6.6.0",
    "pino-pretty": "^4.2.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.20",
    "typeorm-seeding": "^1.6.1"
  }
}
